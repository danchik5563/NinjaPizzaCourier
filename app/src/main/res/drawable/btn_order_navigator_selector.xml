<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <gradient
                android:startColor="#ff9d2c"
                android:endColor="#d78a31"
                android:angle="270" />
            <stroke
                android:width="1dp"
                android:color="#565656" />
            <corners
                android:radius="5dp" />
        </shape>
    </item>

    <item>
        <shape>
            <gradient
                android:startColor="#ff8800"
                android:endColor="#c66900"
                android:angle="270" />
            <stroke
                android:width="1dp"
                android:color="#565656" />
            <corners
                android:radius="5dp" />
        </shape>
    </item>
</selector>