<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <gradient
                android:startColor="#98afba"
                android:endColor="#708792"
                android:angle="270" />
            <stroke
                android:width="1dp"
                android:color="#565656" />
            <corners
                android:radius="5dp" />
        </shape>
    </item>

    <item>
        <shape>
            <gradient
                android:startColor="#849ba6"
                android:endColor="#5c737e"
                android:angle="270" />
            <stroke
                android:width="1dp"
                android:color="#565656" />
            <corners
                android:radius="5dp" />
        </shape>
    </item>
</selector>